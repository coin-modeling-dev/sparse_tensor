{
  "StochasticInventory": {
    "type": "model",
    "style": "stochastic multi-period",
    "containers": ["INVENTORY"],
    "random": ["Demand"],
    "realvariable": ["Balance"],
    "integervariable":["X"]
    "input": {

    },
    "output": {

    },
    "solver": "SMI"
  },
  "INVENTORY": {
    "type": "container",
    "imports": [
      "Balance0",
      "Demand"
    ],
    "exports": [
      "Balance",
      "StockOrder",
      "OrderFulfilled"
    ],
    "definition": {
      "P": {
        "type": "set",
        "ordered": false
      },
      "T": {
        "type": "set",
        "ordered": true,
        "starts": 0
      },
      "StockOrder": {
        "type": "tensor",
        "index": [
          "P",
          "T"
        ]
      },
      "Balance": {
        "type": "tensor",
        "index": [
          "P",
          "T"
        ],
        "definition": {
          "T>0": "Balance[T,P] = Balance[T-1,P] + X[T,P] - D[T,P]",
          "T==0": "Balance[0,P] = Balance0[P]"
        }
      },
      "Balance0": {
        "type": "tensor",
        "index": [
          "P"
        ]
      },
      "Demand": {
        "type": "tensor",
        "index": [
          "T",
          "P"
        ]
      }
    }
  }
}
